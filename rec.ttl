  sh:property [
      a sh:PropertyShape ;
      sh:path brick:hasPoint ;
      sh:class brick:Point ;
      sh:name "has point" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path rec:IPAddress ;
      sh:datatype xsd:string ;
      sh:name "IP address" ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path rec:MACAddress ;
      sh:datatype xsd:string ;
      sh:name "MAC address" ;
    ] ;
  sh:property [
